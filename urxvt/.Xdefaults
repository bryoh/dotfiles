!============================================================================================= General Settings =================
URxvt.termName: rxvt-256-color
xterm*faceName: PowerlineSymbols
xterm*faceSize: 13
URxvt.intensityStyles:	false
URxvt.background:       [1]#000000
URxvt.depth:		32
URxvt.font: xft:Source Code Pro:size=10.5
URxvt.scrollBar:	false
URxvt.cursorColor:	white
URxvt.iso14755: false
URxvt.iso14755_52: false

!Urxvt*geometry:      100x30
Urxvt*highlightSelection: true
Urxvt*transparent: true
Urxvt*shading: 10
Urxvt*tintColor: [1]${xrdb:color1}
Urxvt*blurRadius: 60


!============================================================================================= Extensions 
URxvt.perl-ext-common:	default,matcher,resize-font,url-select,keyboard-select,font-size
URxvt.colorUL:		#4682B4

!============================================================================================= url-select
URxvt.keysym.M-u:	perl:url-select:select_next
URxvt.url-select.launcher: linkhandler
URxvt.url-select.underline: true

!============================================================================================ keyboard-select:
URxvt.keysym.M-Escape: perl:keyboard-select:activate


!====================================================================== ! resize-font
URxvt.resize-font.smaller: C-minus
URxvt.resize-font.bigger: C-plus
URxvt.resize-font.reset: C-equal
URxvt.resize-font.show: C-question

!====================================================================== ! Matcher
URxvt.url-launcher:	linkhandler
URxvt.matcher.button:	1

!====================================================================== ! tabs 
!URxvt.perl-ext-common: ...,tabbed,...


!====================================================================== Rofi =======================
rofi.color-enabled:	true
rofi.color-window:	#000, #000, #000
rofi.color-normal:	#111, #819396, #222, #008ed4, #ffffff
rofi.color-active:	#002b37, #008ed4, #003643, #008ed4, #66c6ff
rofi.color-urgent:	#002b37, #da4281, #003643, #008ed4, #890661

rofi.fake-transparency:	true
rofi.lines:		3
rofi.bw:		0
rofi.opacity:		"8"
rofi.hide-scrollbar:	true
rofi.width:		30
